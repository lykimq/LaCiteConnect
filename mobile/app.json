{
  "expo": {
    "name": "La Cit√© Connect",
    "slug": "la-cite-connect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-mobile.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.laciteconnect.app",
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true,
        "LSApplicationQueriesSchemes": [
          "http",
          "https"
        ],
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      },
      "locales": {
        "en": "./database/locales/ios/en.json",
        "fr": "./database/locales/ios/fr.json"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-mobile.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.laciteconnect.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.egliselacite.com",
              "pathPrefix": "/"
            },
            {
              "scheme": "https",
              "host": "www.egliselacite.com",
              "pathPrefix": "/"
            },
            {
              "scheme": "https",
              "host": "fr.egliselacite.com",
              "pathPrefix": "/"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "com.laciteconnect",
    "plugins": [
      [
        "expo-web-browser",
        {
          "packageName": "com.laciteconnect.app",
          "experimentalLauncherActivity": true
        }
      ],
      "expo-media-library"
    ],
    "extra": {
      "eas": {
        "projectId": "21d26c9d-ba93-4011-8595-17fc0a9a6e33"
      }
    },
    "localization": {
      "defaultLanguage": "en",
      "supportedLanguages": [
        "en",
        "fr"
      ],
      "fallbackLanguage": "en"
    },
    "owner": "lykimq_uyen"
  }
}
